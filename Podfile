target 'MyJson' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MyJson
pod 'SwiftyJSON'

  target 'MyJsonTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyJsonUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
